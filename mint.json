{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Launch Leopard",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "metadata": {
    "og:image": "/images/og_image.png",
    "twitter:image": "/images/og_image.png"
